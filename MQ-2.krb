{"created_at":"2018-11-30T15:20:21.891Z","project_data":{"code":"/**\n * Copyright(C), 2018-2038, KenRobot.com\n * FileName: 我的项目.ino\n * Author: 啃萝卜\n * Create: 2018/11/30\n * Modify: 2018/11/30\n */\n \nvoid setup()\n{\n    Serial.begin(9600);\n    pinMode(13,OUTPUT);\n    pinMode(5,OUTPUT);\n    digitalWrite(5,LOW);\n}\n\nvoid loop()\n{\n   int val;\n    while((val=analogRead(11))<500){\n        if(val>=500) break;\n    }\n   while(1){\n       digitalWrite(13,HIGH);\n        for(int i=200;i<=800;i++){                    //用循环的方式将频率从200HZ 增加到800HZ\n            pinMode(4,OUTPUT);\n            tone(4,i);                            //在四号端口输出频率\n            delay(5);                              //该频率维持5毫秒   \n        }\n        delay(4000);                            //最高频率下维持4秒钟\n        for(int i=800;i>=200;i--){\n            pinMode(4,OUTPUT);\n            tone(4,i);\n            delay(10);\n        }\n    }\n}","hardware":{"board":"ArduinoUNO","components":[],"connections":[]},"mode":"text","software":{"global":{"children":[],"content":[],"enable":true,"name":"group"},"loop":{"children":[],"content":[],"enable":true,"name":"group"},"setup":{"children":[],"content":[],"enable":true,"name":"group"}}},"project_name":"MQ-2","project_type":"local","type":"krobot","updated_at":1543596082}
